{
  "name": "KevinBoxuGao",
  "private": true,
  "version": "1.0.0",
  "description": "Portfolio website",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "webpack --mode production --config webpack.prod.config.js && webpack --mode production --config webpack.server.config.js",
    "start": "webpack-dev-server --open --env.ENVIRONMENT=development --config webpack.dev.config.js --watch --watch-poll",
    "server": "node ./dist/server.js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "author": "Kevin Gao",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^1.0.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.17.1",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^24.9.0",
    "jest-dom": "^4.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.20",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^3.8.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "framer-motion": "^1.10.3",
    "react": "^16.13.1",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.0.1",
    "react-visibility-sensor": "^5.1.1"
  }
}
